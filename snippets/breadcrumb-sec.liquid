<nav class="header-nav">
  <ul>
    {% assign main_menu = linklists.breadcrumb.links %}
    {% for link in main_menu %}
      <li>
        <a href="{{ link.url }}">{{ link.title }}</a>
      {% if link.links != Blank %}
        <ul> 
          {%- for childlink in link.links -%}
          <li><a href="{{ childlink.url }}"> {{ childlink.title | escape }}</a></li>
          {% endfor %}
        </ul>
        {% else %}
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</nav>